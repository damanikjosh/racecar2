<?xml version="1.0"?>
<launch>

  <arg name="map" />
  <!-- Map server -->
  <node pkg="nav2_map_server" exec="map_server" name="map_server" output="screen">
    <param name="yaml_filename" value="$(var map)"/>
  </node>

  <!-- Lifecycle manager -->
  <node pkg="nav2_lifecycle_manager" exec="lifecycle_manager" name="map_lifecycle_manager" output="screen">
    <param name="autostart" value="true"/>
    <param name="node_names" value="['map_server']"/>
  </node>

  <include file="$(find-pkg-share particle_filter)/launch/particle_filter.launch" />

  
</launch>