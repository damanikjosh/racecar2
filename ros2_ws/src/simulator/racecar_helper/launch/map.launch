<?xml version="1.0"?>
<launch>
  <arg name="map" />
  <arg name="gui" default="false" />
  <arg name="world_name" default="$(find-pkg-share racecar_helper)/worlds/map.sdf" />
  <arg name="joystick" default="true" />

  <node pkg="racecar_helper" exec="map2stl.py" name="map2stl" output="screen"
    args="$(var map)" >
  </node>

  <node pkg="rviz2" exec="rviz2" name="rviz2" output="screen" args="-d $(find-pkg-share racecar_helper)/rviz/racecar.rviz">
    <param name="use_sim_time" value="true" />
  </node>

  <!-- ros_gz_sim gz_sim.launch.py -->
  <include file="$(find-pkg-share racecar_gazebo)/launch/empty_world.launch">
    <arg name="gui" value="$(var gui)" />
    <arg name="world_name" value="$(var world_name)"/>
    <arg name="joystick" value="$(var joystick)" />
  </include>
  
  <node pkg="nav2_map_server" exec="map_server" name="map_server" output="screen">
    <param name="use_sim_time" value="true" />
    <param name="yaml_filename" value="$(var map)" />
    <param name="topic" value="map" />
    <param name="frame_id" value="map" />
  </node>fasd

  <node pkg="nav2_lifecycle_manager" exec="lifecycle_manager" name="lifecycle_manager" output="screen">
    <param name="use_sim_time" value="true" />
    <param from="$(find-pkg-share racecar_helper)/config/lifecycle_manager.yaml" />
  </node>

  <node pkg="particle_filter" exec="particle_filter" name="particle_filter" output="screen">
    <param from="$(find-pkg-share racecar_helper)/config/particle_filter.yaml" />
  </node>
  

</launch>